              "expression": "A",
              "filters": {
                "items": [
                  {
                    "id": "service-filter",
                    "key": {
                      "dataType": "string",
                      "id": "service.name--string--tag--false",
                      "isColumn": false,
                      "isJSON": false,
                      "key": "service.name",
                      "type": "tag"
                    },
                    "op": "=",
                    "value": "{{.service.name}}"
                  }
                ],
                "op": "AND"
              },
              "functions": [],
              "groupBy": [
                {
                  "dataType": "string",
                  "id": "payment_type--string--tag--false",
                  "isColumn": false,
                  "isJSON": false,
                  "key": "payment_type",
                  "type": "tag"
                }
              ],
              "having": [],
              "legend": "{{payment_type}}",
              "limit": 10,
              "orderBy": [],
              "queryName": "A",
              "reduceTo": "avg",
              "spaceAggregation": "sum",
              "stepInterval": 60,
              "timeAggregation": "rate"
            }
          ],
          "queryFormulas": []
        },
        "clickhouse_sql": [],
        "id": "payment-requests-query",
